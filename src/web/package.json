{
  "name": "EsselBO",
  "version": "0.0.1",
  "contributors": [
    "Yuri Papouski"
  ],
  "repository": "https://git.oxagile.com/esselgroup/zeez5backoffice",
  "description": "The back office application for Essel corporation",
  "scripts": {
    "dev": "webpack-dev-server --hot --inline --devtool inline-source-map --define __env__=\"'dev'\"",
    "prod": "webpack -p -c -v --display-error-details --display-reasons --define __env__=\"'prod'\"",
    "test:dev": "karma start karma.config.js --browsers Chrome --single-run=false --env=\"'dev'\"",
    "test:prod": "karma start karma.config.js --browsers PhantomJS --env=\"'prod'\""
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  },
  "engines": {
    "npm": "^4.0.3"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "downloadjs": "^1.4.7",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "mobx": "^2.7.0",
    "mobx-react": "^4.0.3",
    "mobx-react-router": "^3.0.0",
    "mobx-utils": "^1.1.2",
    "moment": "^2.18.1",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.2",
    "react-addons-update": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-checkbox-tree": "1.4.1",
    "react-datetime": "^2.8.9",
    "react-dom": "^15.4.1",
