    "mobx-react-devtools": "^4.2.10",
    "mocha": "^3.2.0",
    "path": "^0.12.7",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "directories": {
    "src": "src",
    "dist": "dist",
    "tests": "tests"
  },
  "main": "web/app/app.js",
  "keywords": [
